    Netw <- t(as.matrix(X)) \%*\% as.matrix(X)
    res_all <- lapply(1:(p - 1), function(x) x)
    res_pres <- lapply(res_all, function(x) Perm_j_s(j = x, Netw = Netw, 
        k = k, p = p))
    return(res_pres)
  }
}
